{
  "name": "elbucho/library",
  "description": "Library system for cataloging books and managing reservations",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Hinkson",
      "email": "mjhinkson@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Elbucho\\Library\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Elbucho\\Library\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "ext-pdo": "*",
    "symfony/yaml": "*",
    "elbucho/config": "dev-master",
    "elbucho/database": "dev-master",
    "respect/validation": "2.0.x-dev",
    "slim/slim": "4.x-dev",
    "slim/psr7": "1.1.0",
    "php-di/php-di": "*"
  },
  "require-dev": {
    "behat/behat": ">=3.5",
    "leanphp/behat-code-coverage": ">=3.4"
  }
}